            <xs:attribute name="SharedRoomInd" type="xs:boolean" use="optional">
              <xs:annotation>
                <xs:documentation xml:lang="en">If TRUE, the room requires or has sharing available. Note: Typically used by Hosteliers. TO BE UPDATED...</xs:documentation>
              </xs:annotation>
            </xs:attribute>
          </xs:extension>
