                                  <xs:documentation xml:lang="en">Premodification copy of the hotel reservation.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                            </xs:sequence>
                          </xs:extension>
                        </xs:complexContent>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:extension>
              </xs:complexContent>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="RoutingHops" type="hct:RoutingHopType">
            <xs:annotation>
              <xs:documentation xml:lang="en">A collection of routing hops.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="WrittenConfInst" type="ct:WrittenConfInstType">
