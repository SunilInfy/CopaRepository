          <xs:element minOccurs="0" name="RoomStays" type="RoomStaysType">
            <xs:annotation>
              <xs:documentation xml:lang="en">Collection of room stays.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="Services" type="ServicesType">
            <xs:annotation>
              <xs:documentation xml:lang="en">Collection of services.</xs:documentation>
