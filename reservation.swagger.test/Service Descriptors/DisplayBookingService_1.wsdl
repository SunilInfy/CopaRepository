            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="9" name="RoomRate" type="RoomRateLiteType"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="GuestCounts" type="GuestCountType">
            <xs:annotation>
              <xs:documentation xml:lang="en">Number of guests associated with this reservation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="TimeSpan" type="ct:DateTimeSpanType">
