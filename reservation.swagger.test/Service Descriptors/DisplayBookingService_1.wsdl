            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="BasicPropertyInfo">
            <xs:annotation>
              <xs:documentation xml:lang="en">Basic hotel property information.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:attributeGroup ref="ct:HotelReferenceGroup"/>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="RoomSharesType">
        <xs:annotation>
          <xs:documentation xml:lang="en">A collection of RoomShare.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="unbounded" name="RoomShare">
            <xs:annotation>
              <xs:documentation xml:lang="en">Guests to share a room.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" name="GuestRPHs">
                  <xs:annotation>
                    <xs:documentation xml:lang="en">Collection of Guest RPH items.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="99" name="GuestRPH">
                        <xs:annotation>
                          <xs:documentation xml:lang="en">References one of the guest(s) staying in this room.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:simpleContent>
                            <xs:extension base="st:RPH_Type">
                              <xs:attributeGroup ref="ct:DateTimeSpanGroup"/>
