                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="RPH" type="st:RPH_Type" use="required">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">A unique reference to the guest ID.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
              <xs:attribute name="BillingCode" type="xs:string" use="required">
                <xs:annotation>
                  <xs:documentation xml:lang="en">The individual billing code that applies to a set of instructions that are routed to a designated folio.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="BillingType" type="st:OTA_CodeType" use="optional">
