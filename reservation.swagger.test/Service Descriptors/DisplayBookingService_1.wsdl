              </xs:element>
            </xs:sequence>
            <xs:element name="Errors" type="ct:ErrorsType"/>
          </xs:choice>
        </xs:sequence>
        <xs:attributeGroup ref="ct:OTA_PayloadStdAttributes"/>
        <xs:attribute name="ResResponseType" type="st:TransactionStatusType"/>
