            </xs:complexType>
          </xs:element>
          <xs:element maxOccurs="99" minOccurs="0" name="PassengerInfo" type="RailPassengerCategoryDetailType">
            <xs:annotation>
              <xs:documentation xml:lang="en">Passenger information for a reservation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="PaymentRules">
            <xs:annotation>
              <xs:documentation xml:lang="en">A collection of payment rules associated with this rail reservation.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="9" name="PaymentRule" type="ct:MonetaryRuleType">
