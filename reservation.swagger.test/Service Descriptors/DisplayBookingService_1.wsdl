            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="5" minOccurs="0" name="Email" type="EmailType">
            <xs:annotation>
              <xs:documentation xml:lang="en">Electronic email addresses, in IETF specified format.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="5" minOccurs="0" name="URL" type="URL_Type">
