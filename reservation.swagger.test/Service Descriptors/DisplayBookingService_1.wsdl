    </xs:schema>
    <xs:schema xmlns="http://www.opentravel.org/OTA/2003/05/HotelReservation" xmlns:ct="http://www.opentravel.org/OTA/2003/05/CommonTypes" xmlns:hct="http://www.opentravel.org/OTA/2003/05/HotelCommonTypes" xmlns:pro="http://www.opentravel.org/OTA/2003/05/Profile" xmlns:st="http://www.opentravel.org/OTA/2003/05/SimpleTypes" xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" id="OTA2003A2011A" targetNamespace="http://www.opentravel.org/OTA/2003/05/HotelReservation" version="6.001">
