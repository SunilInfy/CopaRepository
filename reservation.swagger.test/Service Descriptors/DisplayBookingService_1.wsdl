                  <xs:documentation xml:lang="en">Fare adjustments based on accommodation type.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:complexContent>
                    <xs:extension base="CodeAmountType"/>
                  </xs:complexContent>
                </xs:complexType>
              </xs:element>
              <xs:element maxOccurs="99" minOccurs="0" name="FareQualifierAdjustment" type="CodeAmountType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Other fare qualifier adjustments.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element maxOccurs="99" minOccurs="0" name="DiscountClass">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Fare promotions and other qualified discounts.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attributeGroup ref="ct:DiscountInfoGroup"/>
                </xs:complexType>
              </xs:element>
              <xs:element maxOccurs="99" minOccurs="0" name="AncillaryCharge" type="CodeAmountType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Ancillary product and service charges.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element maxOccurs="99" minOccurs="0" name="Fee" type="CodeAmountType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Applicable Fee code and associated amount.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element maxOccurs="9" minOccurs="0" name="Tax" type="CodeAmountType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Applicable tax code and associated amount.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
